@model IEnumerable<TedAzApp.DataContext.Models.FileMentions>
@{
    ViewData["Title"] = "File Add";
}

<h3>Select file to Upload</h3>
<form asp-action="AddFileMentions" asp-controller="Admin" method="post" enctype="multipart/form-data">
    <input type="file" name="uploadedFile" /><br>
    <input type="submit" value="Upload" />
</form>

